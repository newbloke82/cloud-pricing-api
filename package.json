{
  "name": "cloud-pricing-api",
  "version": "0.3.18",
  "description": "Cloud Pricing API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "postbuild": "copyfiles -u 1 src/views/**/** dist && copyfiles -u 1 src/public/**/** dist",
    "start": "node dist/server.js",
    "db:setup": "node dist/cmd/dbSetup.js",
    "data:scrape": "node dist/cmd/dataScrape.js",
    "data:dump": "node dist/cmd/dataDump.js",
    "data:download": "node dist/cmd/dataDownload.js",
    "data:load": "node dist/cmd/dataLoad.js",
    "data:status": "node dist/cmd/dataStatus.js",
    "job:init": "npm run db:setup && npm run job:update",
    "job:update": "node dist/cmd/jobUpdate.js",
    "db:setup:dev": "ts-node src/cmd/dbSetup.ts",
    "data:scrape:dev": "ts-node src/cmd/dataScrape.ts",
    "data:dump:dev": "ts-node src/cmd/dataDump.ts",
    "data:download:dev": "ts-node src/cmd/dataDownload.ts",
    "data:load:dev": "ts-node src/cmd/dataLoad.ts",
    "data:status:dev": "ts-node src/cmd/dataStatus.ts",
    "job:init:dev": "npm run db:setup:dev && npm run job:update:dev",
    "job:update:dev": "ts-node src/cmd/jobUpdate.ts",
    "lint": "eslint --ext .ts,.js --ignore-path .gitignore .",
    "format": "prettier --write 'src/**/*.{js,ts}'"
  },
  "author": "Infracost",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "https://raw.githubusercontent.com/infracost/cloud-pricing-api/master/LICENSE"
    }
  ],
  "dependencies": {
    "@graphql-tools/schema": "^10.0.0",
    "@graphql-tools/utils": "^10.0.6",
    "apollo-server-core": "^3.12.1",
    "apollo-server-express": "^3.12.1",
    "apollo-server-plugin-base": "^3.5.1",
    "async-mutex": "^0.4.0",
    "axios": "^1.5.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "glob": "^10.3.4",
    "googleapis": "^126.0.1",
    "graphql": "^16.8.0",
    "lodash": "^4.17.21",
    "mingo": "^6.4.4",
    "node-cache": "^5.1.2",
    "node-fetch": "2.6.9",
    "pg": "^8.11.3",
    "pg-copy-streams": "^6.0.6",
    "pg-format": "^1.0.4",
    "pino": "8.15.0",
    "pino-http": "^8.5.0",
    "pino-pretty": "^10.2.0",
    "prettier": "^3.0.3",
    "progress": "^2.0.3",
    "tmp": "^0.2.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/node": "^7.22.15",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.5.9",
    "@types/node-fetch": "^2.6.4",
    "@types/pg": "^8.10.2",
    "@types/pg-copy-streams": "^1.2.1",
    "@types/pg-format": "^1.0.2",
    "@types/pino-http": "^5.8.1",
    "@types/progress": "^2.0.5",
    "@types/tmp": "^0.2.1",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infracost/cloud-pricing-api.git"
  }
}
